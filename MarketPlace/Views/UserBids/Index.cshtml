@model IEnumerable<MarketPlace.Models.BidViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>My Bids on Projects</h2>

<p>
    @*@Html.ActionLink("Create New", "Create")*@
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProjectId)
